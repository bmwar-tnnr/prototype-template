{
  "name": "@tennr/lasso",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "exports": {
    "./*": {
      "types": "./dist/src/*/*.d.ts",
      "import": "./dist/src/*/*.js",
      "default": "./dist/src/*/*.js"
    },
    "./utils/*": {
      "types": "./dist/src/utils/*.d.ts",
      "import": "./dist/src/utils/*.js",
      "default": "./dist/src/utils/*.js"
    },
    "./hooks/*": {
      "types": "./dist/src/hooks/*.d.ts",
      "import": "./dist/src/hooks/*.js",
      "default": "./dist/src/hooks/*.js"
    },
    "./theme.css": "./src/theme.css",
    "./dist/src/styles.css": "./dist/src/styles.css",
    "./assets/*": "./dist/assets/*"
  },
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "build": "pnpm tailwind && tsc -b tsconfig.build.json && pnpm copy-assets",
    "watch": "concurrently \"tsc -b --watch --preserveWatchOutput\" \"pnpm tailwind --watch\" \"pnpm copy-assets --watch\"",
    "copy-assets": "cp -r assets dist/assets",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "tailwind": "pnpm exec tailwindcss -i ./src/styles.css -o ./dist/src/styles.css --minify"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@number-flow/react": "^0.5.9",
    "arktype": "^2.1.20",
    "@radix-ui/react-accordion": "^1.2.10",
    "@radix-ui/react-alert-dialog": "^1.1.13",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.9",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-collapsible": "^1.1.10",
    "@radix-ui/react-context-menu": "^2.2.14",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.13",
    "@radix-ui/react-progress": "^1.1.6",
    "@radix-ui/react-radio-group": "^1.3.6",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-separator": "^1.1.6",
    "@radix-ui/react-slider": "^1.3.4",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-toggle": "^1.1.8",
    "@radix-ui/react-toggle-group": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@tanstack/react-table": "^8.21.3",
    "chrono-node": "^2.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "motion": "^12.6.3",
    "next-themes": "^0.4.6",
    "parsimmon": "^1.18.1",
    "phosphor-react": "^1.4.1",
    "react-hook-form": "^7.56.4",
    "react-day-picker": "^9.9.0",
    "react-resizable-panels": "^3.0.5",
    "recharts": "2.15.4",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.6",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@iconify/react": "^4.1.1",
    "@phosphor-icons/react": "^2.1.10",
    "@storybook/react": "^10.1.10",
    "@tailwindcss/cli": "^4.1.11",
    "@types/parsimmon": "^1.10.9",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2"
  }
}
